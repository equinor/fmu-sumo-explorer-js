    "lru-cache": "^11.2.2",
    "mocha": "^11.7.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.1",
    "eslint": "^9.39.1",
    "jsdoc": "^4.0.5",
    "jsdoc-to-markdown": "^9.1.3"
  }
